@article{MWPC2,
title = {Development of a multiwire proportional chamber with good tolerance to burst hits},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {999},
pages = {165228},
year = {2021},
issn = {0168-9002},
doi = {https://doi.org/10.1016/j.nima.2021.165228},
url = {https://www.sciencedirect.com/science/article/pii/S0168900221002126},
author = {N. Teshima and M. Aoki and Y. Higashino and H. Ikeuchi and K. Komukai and D. Nagao and Y. Nakatsugawa and H. Natori and Y. Seiya and N.M. Truong and K. Yamamoto},
keywords = {Multiwire proportional chamber, High voltage switching, Magnetic spectrometer, J-PARC MLF},
abstract = {The DeeMe experiment to search for muon-to-electron conversions with a sensitivity 10–100 times better than those achieved by previous experiments is in preparation at the Japan Proton Accelerator Research Complex. The magnetic spectrometer used by the DeeMe experiment consists of an electromagnet and four multiwire proportional chambers (MWPCs). The newly developed MWPCs are operated with a high voltage (HV) switching technique and have good burst-hit tolerance. In this article, the final designs of the MWPCs, amplifiers for readout, and HV switching modules are described. Additionally, some results of MWPC performance evaluation are presented.}
}
